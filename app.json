        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.identifier.name"
    },
    "extra": {
      "eas": {
        "projectId": "Your project id"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}